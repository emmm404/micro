{
    "name": "est-pc",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "npm run clean && npm run cluster-build",
        "cluster-build": "node ./build/scripts/clusterBuild.js --max-old-space-size=16384",
        "clean": "rimraf ./dist",
        "dll": "webpack --mode=production --config ./build/webpack.dll.js",
        "clean:plugin": "rimraf ./client/plugins/publish/plugin.js ./client/plugins/publish/plugin.css",
        "plugin": "npm run clean:plugin && webpack --config ./build/webpack.plugin.config.js",
        "plugin-auto": "node ./build/scripts/autodeploy.js",
        "micro:dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.micro.config.js --progress",
        "micro:build": "cross-env NODE_ENV=production webpack --config ./build/webpack.micro.config.js",
        "micro:build:test": "cross-env NODE_ENV=production BUILD_ENV=test webpack --config ./build/webpack.micro.config.js",
        "dev:app": "cross-env MODULE=app npm run micro:dev",
        "build:app": "cross-env MODULE=app npm run micro:build",
        "build:app:test": "cross-env MODULE=app npm run micro:build:test",
        "dev:beforeManage": "cross-env MODULE=beforeManage npm run micro:dev",
        "build:beforeManage": "cross-env MODULE=beforeManage npm run micro:build",
        "build:beforeManage:test": "cross-env MODULE=beforeManage npm run micro:build:test",
        "dev:centerManage": "cross-env MODULE=centerManage npm run micro:dev",
        "build:centerManage": "cross-env MODULE=centerManage npm run micro:build",
        "build:centerManage:test": "cross-env MODULE=centerManage npm run micro:build:test",
        "dev:afterManage": "cross-env MODULE=afterManage npm run micro:dev",
        "build:afterManage": "cross-env MODULE=afterManage npm run micro:build",
        "build:afterManage:test": "cross-env MODULE=afterManage npm run micro:build:test",
        "dev:exitManage": "cross-env MODULE=exitManage npm run micro:dev",
        "build:exitManage": "cross-env MODULE=exitManage npm run micro:build",
        "build:exitManage:test": "cross-env MODULE=exitManage npm run micro:build:test"
    },
